{
  "name": "@marsupialdev/joey",
  "version": "0.1.18",
  "description": "A framework for Cloudflare Workers",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig-build.json",
    "prepub": "npm run build && npm version patch && node scripts/create-dist-package-json.js",
    "pub": "npm publish ./lib",
    "pretest": "tsc -p tsconfig.json",
    "test": "c8 ava --verbose",
    "test:watch": "c8 ava --verbose --watch",
    "watch": "watch 'npm run build && node scripts/create-dist-package-json.js' ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/marsupialdev/joey.git"
  },
  "publishConfig": {
    "@marsupialdev:registry": "https://gitlab.com/api/v4/projects/30124177/packages/npm/"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "framework",
    "kv",
    "backend",
    "typescript",
    "http",
    "json",
    "router"
  ],
  "author": "Abdul Jabbar",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/marsupialdev/joey/issues"
  },
  "homepage": "https://gitlab.com/marsupialdev/joey#readme",
  "devDependencies": {
    "@ava/typescript": "^2.0.0",
    "@cloudflare/workers-types": "^3.0.0",
    "ava": "^3.15.0",
    "c8": "^7.10.0",
    "cross-fetch": "^3.1.4",
    "esm": "^3.2.25",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4",
    "watch": "^0.13.0"
  }
}
