{
  "name": "@marsupialdev/joey",
  "version": "0.1.39",
  "main": "./Routing/index.js",
  "description": "A framework for Cloudflare Workers",
  "scripts": {
    "build": "npm run cleanup && tsc -p tsconfig-build.json && node scripts/create-dist-package-json.js",
    "build:prod": "npm run cleanup && tsc -p tsconfig-build.json",
    "prepub": "npm run build:prod && npm version patch && node scripts/create-dist-package-json.js",
    "pub": "npm publish ./lib",
    "test": "c8 ava",
    "test:watch": "c8 ava --watch",
    "watch": "watch 'npm run build' ./src",
    "cleanup": "rm -rf ./lib ./lib-test ./coverage"
  },
  "exports": {
    ".": "./Routing",
    "./Transforms": "./Transforms",
    "./Crypto": "./Crypto",
    "./Utilities": "./Utilities"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/marsupialdev/joey.git"
  },
  "publishConfig": {
    "@marsupialdev:registry": "https://gitlab.com/api/v4/projects/30124177/packages/npm/"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "framework",
    "kv",
    "backend",
    "typescript",
    "http",
    "json",
    "router"
  ],
  "author": "Abdul Jabbar",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/marsupialdev/joey/issues"
  },
  "homepage": "https://gitlab.com/marsupialdev/joey#readme",
  "devDependencies": {
    "@ava/typescript": "^2.0.0",
    "@cloudflare/workers-types": "^3.2.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "ava": "^3.15.0",
    "c8": "^7.10.0",
    "cross-fetch": "^3.1.4",
    "eslint": "^8.5.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "esm": "^3.2.25",
    "form-data": "^4.0.0",
    "lodash": "^4.17.21",
    "performance-now": "^2.1.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4",
    "watch": "^0.13.0"
  }
