{
  "name": "joeycf",
  "version": "0.3.25",
  "main": "./src/Core/index.js",
  "type": "module",
  "description": "A framework for Cloudflare Workers",
  "scripts": {
    "build:src": "tsc -p tsconfig.build.json",
    "build:bin": "tsc -p tsconfig.bin.json --noEmit && node scripts/build-bin.js",
    "build": "npm run cleanup && npm run build:src && npm run build:bin && node scripts/create-dist-package.js",
    "prepub": "npm run build && npm version patch && node scripts/create-dist-package.js",
    "prepub:minor": "npm run build && npm version minor && node scripts/create-dist-package.js",
    "pub": "npm publish ./lib",
    "pub:minor": "npm pub",
    "test": "tsc -p tsconfig.test.json && c8 ava --config ava.config.js && rm -rf libTest",
    "watch": "watch 'npm run build' ./src ./bin",
    "watch:src": "watch 'npm run build:src' ./src",
    "watch:bin": "watch 'npm run build:bin' ./bin",
    "cleanup": "rm -rf ./lib ./coverage",
    "madge": "npm run build && mkdir -p .graphviz && madge --image .graphviz/src.svg --exclude '^(test)\\.ts$' src",
    "madge:lib": "npm run build && mkdir -p .graphviz && madge --extensions js --image .graphviz/lib.svg lib",
    "madge:lib-dts": "npm run build && mkdir -p .graphviz && madge --extensions ts --image .graphviz/lib-dts.svg lib"
  },
  "bin": {
    "joeycf": "./bin/joeycf.js"
  },
  "exports": {
    ".": {
      "import": "./src/Core/joey.js"
    },
    "./Logger": {
      "import": "./src/Logger/index.js"
    },
    "./Transforms": {
      "import": "./src/Transforms/index.js"
    },
    "./Crypto": {
      "import": "./src/Crypto/index.js"
    },
    "./Utilities": {
      "import": "./src/Utilities/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/Abdul-Jabbar/joeycf.git"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "framework",
    "kv",
    "backend",
    "edge",
    "typescript",
    "http",
    "json",
    "router"
  ],
  "author": "Abdul Jabbar",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/Abdul-Jabbar/joeycf/issues"
  },
  "homepage": "https://gitlab.com/Abdul-Jabbar/joeycf#readme",
  "engines": {
    "node": ">=18"
  },
  "peerDependencies": {
    "@cloudflare/workers-types": "^4.20240222.0"
  },
  "bundledDependencies": [
    "@cloudflare/workers-types",
    "fast-glob",
    "esbuild"
  ],
  "devDependencies": {
    "@ava/typescript": "^4.1.0",
    "@types/lodash": "^4.14.202",
    "@types/node": "^18.19.8",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "ava": "^5.3.1",
    "c8": "^8.0.1",
    "camelcase": "^7.0.1",
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-ava": "^14.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "form-data": "^4.0.0",
    "handlebars": "^4.7.8",
    "madge": "^6.1.0",
    "typescript": "^5.3.3",
    "undici": "^5.28.2",
    "watch": "^1.0.2",
    "wrangler": "^3.23.0"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20240222.0",
    "ajv": "^8.12.0",
    "ajv-cli": "^5.0.0",
    "ajv-errors": "^3.0.0",
    "esbuild": "^0.19.11",
    "fast-glob": "^3.3.2",
    "lodash": "^4.17.21",
    "maxage": "^2.0.4",
    "yaml": "^2.3.4"
  },
  "bundleDependencies": [
    "fast-glob",
    "esbuild"
  ]
}
