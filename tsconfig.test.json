{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "skipLibCheck": true,
    "declaration": true,
    "outDir": "libTest",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "sourceMap": false,
    "removeComments": true,
    "types": ["@cloudflare/workers-types", "@types/node"],
  },
  //"include": ["src", "bin"], // TODO
  "include": ["src"],
}

