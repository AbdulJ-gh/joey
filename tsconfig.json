{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "lib": ["esnext"],
    "declaration": true,
    "outDir": "lib-test",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "sourceMap": false,
    "types": ["@cloudflare/workers-types"],
  },
  "include": [ "src", "testUtils"]
}
